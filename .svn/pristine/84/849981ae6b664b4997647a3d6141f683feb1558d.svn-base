#revision_banner {width:890px; height:400px; overflow:hidden; position:relative; z-index:0; float:right}
#revision_banner ul {height:400px; position:absolute;}
#revision_banner ul li {float:left; width:890px; height:400px; overflow:hidden; position:relative; z-index:1;}
#revision_banner ul li div {position:absolute; overflow:hidden;}
#revision_banner .btnBg {position:absolute; width:890px; height:20px; left:0; bottom:0; background:#000; z-index:1;}
#revision_banner .btn {position:absolute; width:890px; height:10px; padding:5px 10px; right:0; bottom:0; text-align:right; z-index:1;}
#revision_banner .btn span {display:inline-block; _display:inline; _zoom:1; width:25px; height:10px; _font-size:0; margin-left:5px; cursor:pointer; z-index:1; background:#fff;}
#revision_banner .btn span.on {background:#fff; z-index:1;}
#revision_banner .preNext {width:45px; height:100px; z-index:1; position:absolute; top:150px; background:url(../img/sprite.png) no-repeat 0 0; cursor:pointer; z-index:100; display:block}
#revision_banner .pre {left:0; z-index:100;}
#revision_banner .next {right:0; z-index:100; background-position:right top;}



