body {margin:0px; font-size:12px; font-family:Arial, Helvetica, sans-serif; background:url(../images/n_02.jpg) repeat-x}
div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,blockquote,p{padding:0px; margin:0px;}
