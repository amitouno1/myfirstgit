<script language="javascript" type="text/javascript"> 
	var interval = 1000; 
	function ShowCountDown(year,month,day,divname) 
	{ 
		var now = new Date();
		
		//alert(now.getDate());
		
		var endDate = new Date(year, month-1, day); 
		var leftTime=endDate.getTime()-now.getTime(); 
		var leftsecond = parseInt(leftTime/1000); 
		//var day1=parseInt(leftsecond/(24*60*60*6)); 
		var day1=Math.floor(leftsecond/(60*60*24)); 
		var hour=Math.floor((leftsecond-day1*24*60*60)/3600); 
		var minute=Math.floor((leftsecond-day1*24*60*60-hour*3600)/60); 
		var second=Math.floor(leftsecond-day1*24*60*60-hour*3600-minute*60); 
		var cc = document.getElementById(divname); 
		//cc.innerHTML =year+"年"+month+"月"+day+"日还有："+day1+"天"+hour+"小时"+minute+"分"+second+"秒"; 
		cc.innerHTML ="还剩："+day1+"天"+hour+"小时"+minute+"分"+second+"秒"; 
	} 
	
	function ShowCountDown(divname) 
	{ 
		var now = new Date();
		var year=now.getFullYear();
		var month=now.getMonth();
		var day=now.getDate();

		var endDate = new Date(year, month, day+7); 
		var leftTime=endDate.getTime()-now.getTime(); 
		var leftsecond = parseInt(leftTime/1000); 
		//var day1=parseInt(leftsecond/(24*60*60*6)); 
		var day1=Math.floor(leftsecond/(60*60*24)); 
		var hour=Math.floor((leftsecond-day1*24*60*60)/3600); 
		var minute=Math.floor((leftsecond-day1*24*60*60-hour*3600)/60); 
		var second=Math.floor(leftsecond-day1*24*60*60-hour*3600-minute*60); 
		var cc = document.getElementById(divname); 
		cc.innerHTML ="还剩："+day1+"天"+hour+"小时"+minute+"分"+second+"秒"; 
	} 
	
	
	//window.setInterval(function(){ShowCountDown(2015,4,20,'divdown1');}, interval); 
	window.setInterval(function(){ShowCountDown('divdown1');}, interval); 
	window.setInterval(function(){ShowCountDown('divdown2');}, interval); 
	window.setInterval(function(){ShowCountDown('divdown3');}, interval); 
	window.setInterval(function(){ShowCountDown('divdown4');}, interval); 
	
	
	function openWindows(url)
	{
		window.open(url);
	}
	
	
</script> 

